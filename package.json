{
  "name": "annalise-exercise",
  "scripts": {
    "prisma:dev:deploy": "prisma migrate deploy",
    "db:dev:rm": "docker compose rm dev-db -s -f -v",
    "db:dev:up": "docker compose up dev-db -d",
    "prisma:test:deploy": "dotenv -e .env.test -- prisma migrate deploy",
    "db:test:rm": "docker compose rm test-db -s -f -v",
    "db:test:up": "dotenv -e .env.test -- docker compose up test-db -d",
    "db:test:restart": "yarn db:test:rm && yarn db:test:up && sleep 1 && yarn prisma:test:deploy",
    "lint": "eslint --ext .js,.ts functions",
    "build": "tsc",
    "start": "node-dev src/index.ts",
    "test": "dotenv -e .env.test -- jest",
    "test:watch": "dotenv -e .env.test -- jest --watch",
    "test:cov": "dotenv -e .env.test -- jest --coverage",
    "pretest:e2e": "yarn db:test:restart",
    "test:e2e": "dotenv -e .env.test -- jest --watch --no-cache"
  },
  "engines": {
    "node": "18"
  },
  "main": "lib/index.js",
  "dependencies": {
    "@hapi/boom": "^10.0.0",
    "@koa/cors": "^2.2.3",
    "@koa/router": "^12.0.0",
    "@prisma/client": "^4.8.1",
    "argon2": "^0.30.3",
    "axios": "^1.2.2",
    "busboy": "^1.6.0",
    "directory-import": "^2.3.1",
    "dotenv": "^16.0.3",
    "form-data": "^4.0.0",
    "koa": "^2.13.4",
    "koa-body": "^6.0.1",
    "koa-busboy": "^1.2.0",
    "koa-helmet": "^6.1.0",
    "koa-passport": "^5.0.0",
    "passport-jwt": "^4.0.1",
    "prisma": "^4.8.1",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/busboy": "^1.5.0",
    "@types/jest": "^28.1.8",
    "@types/koa": "^2.13.5",
    "@types/koa__cors": "^3.3.0",
    "@types/koa__router": "^8.0.11",
    "@types/koa-passport": "^4.0.3",
    "@types/passport-jwt": "^3.0.8",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "dotenv-cli": "^6.0.0",
    "eslint": "^8.9.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-import": "^2.25.4",
    "fs-extra": "^11.1.0",
    "jest": "^28.1.3",
    "node-dev": "^7.4.3",
    "pactum": "^3.3.2",
    "prettier": "^2.7.1",
    "supertest": "^6.3.3",
    "ts-jest": "^28.0.7",
    "ts-node": "^10.9.1",
    "typescript": "^4.5.4"
  }
}
